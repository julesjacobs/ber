let rec fold_left f acc xs =
  match xs with
  | Nil -> acc
  | Cons (x, xs') -> fold_left f (f acc x) xs'

> val fold_left : ('a -> 'b -> 'a) -> 'a -> 'b list -> 'a

let sum_bool_list xs =
  fold_left (fun (a : int) (b : int) -> a) 0 xs

